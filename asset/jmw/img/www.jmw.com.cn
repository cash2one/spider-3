						<script>
							function subMess(obj){
								if(obj.attr("message_pop_up") ==1)
								{
									alert("正在提交中。。请稍后");
									return false;
								}
								var message1 = $("#souhangye").val();
								var message2 = $("#moneys").val();
								var message3 = $("#address").val();
								var message = "私人订制服务,"+message1+'行业,'+message2+','+message3;
								var MCeng_lasturl_1 = $("#MCeng_lasturl_3").val();
								var MCeng_fromurl_1 = $("#MCeng_fromurl_3").val();
								
								var name = $("#myName").val();
								var phone = $("#myPhone").val();
								var target_id = $('#pids').val();
								var origin = $('#sub_origin').val();
								var sex = $('#sub_sex').val();
								if(message1 == "" || message1 == "输入想投资的行业"){
									alert("投资行业不能为空！");
									return false;	
								}else if(message2 == "" || message2 == "可投资金额"){
									alert("可投资金额不能为空！");
									return false;
								}else if(message3 == "" || message3 == "我的投资的商圈"){
									alert("我的投资的商圈不能为空！");
									return false;
								}else if(name == "" || name == "我的名字"){
									alert("姓名不能为空！");
									return false;
								}else if(phone == "" || phone == "我的手机号码"){
									alert("联系方式不能为空！");  
									return false ; 
								}else if(!phone.match(/^(13[0-9]|14[0-9]|15([0-9])|18([0-9])|17([0-9]))[0-9]{8}$/)) {
									alert("请输入正确的手机号码！");
									return false ;
								}else{
									obj.attr("message_pop_up",1);

									function show()
									{
										obj.attr("message_pop_up",0);
									} 
									window.setTimeout(show,2000);
									$.ajax({
										type:"get",
										url:"http://liuyan.jmw.com.cn/message/v9_message_check.php",
										dataType:"jsonp",
										jsonp:"callback",
										data:{sex:sex,name:name,telephone:phone,message:message,target_id:target_id,origin:origin,MFoot_lasturl:MCeng_lasturl_1,MFoot_fromurl:MCeng_fromurl_1},
										success:function(html){
											obj.attr("message_pop_up",0);
											if(html.status == "login"){
												alert("留言成功！");
												window.location.reload();
											}else if(html.status == "unlogin"){
												$("#cover-Black").show();
												$("#rightNow3").show();
												v9_Auto_trigger();
												$("#first_message_id").val(html.id);
												$("#first_telephone").val(phone);
											}
										}
									});

								}	
							}
						
						</script>
						

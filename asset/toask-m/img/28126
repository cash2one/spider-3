				var sh = document.documentElement.scrollTop || document.body.scrollTop;
		
				if(fixObjPh <= sh && !ie6) {
					fixObj.addClass('fm_locate').css('width', origWidth+'px');
				} else {
					fixObj.removeClass('fm_locate');
				}
			});		

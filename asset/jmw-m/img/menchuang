	<script>
	function publish_message(target_id,origin,message){
		var target_id = target_id;
		var origin = origin;
		var message = message;
		if(origin == 12){
			$('#f_title').html('在线咨询');
		}else if(origin == 13){
			$('#f_title').html('询加盟底价');
		}else if(origin == 14){
			$('#f_title').html('创业意向登记');
		}else if(origin == 15){
			$('#f_title').html('立享加盟优惠');
		}
		$('#target_id').val(target_id);
		$('#f_origin').val(origin);
		$('#f_content').val(message);
		
		$('.black-all5').show();
		$('.tm_bg').show();
		
	}
	function zhaoshang_message(target_id,origin,message){
		var target_id = target_id;
		var origin = origin;
		var message = message;
		if(origin == 16){
			$('#z_title').html('我要招商');
		}else if(origin == 17){
			$('#z_title').html('我要发布');
		}
		$('#z_target_id').val(target_id);
		$('#z_origin').val(origin);
		$('#z_content').val(message);
		
		$('.black-all6').show();
		$('.tm_bg').show();
		
	}
	function dongtai_message(target_id,origin,message){
		var target_id = target_id;
		var origin = origin;
		var message = message;

		$('#z_title').html('我要查看');
		$('#z_target_id').val(target_id);
		$('#z_origin').val(origin);
		$('#z_content').val(message);
		
		$('.black-all6').show();
		$('.tm_bg').show();
		
	}
	</script>
   

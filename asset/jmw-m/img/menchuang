    <script>
        $(document).scroll(function() {
            var top = $(document).scrollTop();
            if(top==0){
                $(".returntop").hide();
            }else{
                $(".returntop").show();
            }
        })
        $(".returntop").click(function(){
            $({aa:$(window).scrollTop()}).animate({aa:0},{duration:1200,step:function(){
                $(window).scrollTop(this.aa);
            }})
        })
    </script>    <!--行业站公共底部 end-->

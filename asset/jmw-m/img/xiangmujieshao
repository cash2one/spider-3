	<script type="text/javascript"> 
	//验证码变化
	/*function t1(){
		$("#vailcode").attr("src","http://m.jmw.com.cn/code.php?rand"+Math.random());
		var session_code = context.Session["Checkcode"].ToString();
		alert(session_code);
		return false;
